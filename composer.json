{
    "name": "lukasccb/filament-balance-header",
    "description": "Show balance via API inside Filament",
    "license": "MIT",
    "version": "1.0.3",
    "keywords": [
        "api balance",
        "filament",
        "laravel-filament"
    ],
    "authors": [
        {
            "name": "Lucas Silva",
            "email": "contato@webzow.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^3.0-stable"
    },
    "require-dev": {
        "laravel/pint": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "lukasccb\\FilamentBalanceHeader\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\lukasccb\\FilamentBalanceHeader\\FilamentBalanceHeaderServiceProvider"
            ]
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint"
    }
}
